# API Configuration
VITE_API_URL=http://localhost:5000/api

# Maps Configuration
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Blockchain Configuration
VITE_CONTRACT_ADDRESS=deployed_contract_address_here
VITE_BLOCKCHAIN_NETWORK=localhost
VITE_ETHEREUM_RPC_URL=http://localhost:8545

# App Configuration
VITE_APP_NAME=NyayChain
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Blockchain-Powered Civic Governance Platform

# Environment
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_BLOCKCHAIN=true
VITE_ENABLE_MAPS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=false

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
VITE_MIXPANEL_TOKEN=your_mixpanel_token_here

# Error Tracking (Optional)
VITE_SENTRY_DSN=your_sentry_dsn_here

# CDN Configuration (Optional)
VITE_CDN_URL=https://cdn.nyaychain.com

# Development Configuration
VITE_DEBUG_MODE=true
VITE_MOCK_API=false
